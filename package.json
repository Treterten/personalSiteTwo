{
  "name": "personalsitetwo",
  "version": "1.0.0",
  "description": "My personal site based on the mockup that I created in Adobe XD",
  "main": "index.js",
  "scripts": {
    "test": "jest ./client/site",
    "test:update": "jest ./client/site --updateSnapshot",
    "start": "node ./server/index.js",
    "start:dev": "nodemon ./server/index.ts",
    "build:dev": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "build:server": "tsc --project ./server"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Treterten/personalSiteTwo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Treterten/personalSiteTwo/issues"
  },
  "homepage": "https://github.com/Treterten/personalSiteTwo#readme",
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^4.41.26",
    "express": "^4.17.1",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "webpack": "^5.27.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.13.10",
    "@types/enzyme": "^3.10.8",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "style-loader": "^2.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack-cli": "^4.5.0"
  }
}
